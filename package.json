{
	"name": "zurg-webdav-proxy",
	"version": "1.0.0",
	"description": "Cloudflare Worker to proxy Zurg HTML endpoint and link the contents of .STRM files",
	"main": "index.js",
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"dev:local": "wrangler dev --config wrangler.local.toml --live-reload",
		"deploy:local": "wrangler deploy --config wrangler.local.toml"
	},
	"keywords": [
		"cloudflare",
		"worker",
		"webdav",
		"zurg",
		"infuse",
		"proxy"
	],
	"author": "andesco",
	"license": "MIT",
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20241127.0",
		"wrangler": "^4.43.0"
	},
	"dependencies": {
		"esbuild": "^0.25.11"
	}
}
